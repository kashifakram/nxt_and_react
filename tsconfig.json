{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/css/*": ["app/styles/*"],
      "@/fon/*": ["app/fonts/*"],
      "@/cont/*": ["app/contexts/*"],
      "@/comps/*": ["app/_components/*"],
      "@/p/*": ["public/*"]
    },
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
, "app/_components/ui/Tasks.jsx", "app/_components/TaskList.jsx", "app/_components/reducer.jsx"    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}